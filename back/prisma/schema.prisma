generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id       Int      @id @default(autoincrement())
  active   Boolean? @default(true)
  createAt DateTime? @default(now())
  email    String   @unique
  name     String
  password String
}
